<Application
    x:Class="ReadyOrNotOpenMic.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ReadyOrNotOpenMic">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="/src/UI/Dictionaries/Thickness.xaml" />
                <ResourceDictionary Source="/src/UI/Dictionaries/Strings.xaml" />
                <ResourceDictionary Source="/src/UI/Dictionaries/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <!--  Breakpoints  -->
            <x:Double x:Key="Breakpoint640Plus">641</x:Double>
            <x:Double x:Key="Breakpoint1280Plus">1281</x:Double>
        </ResourceDictionary>
    </Application.Resources>
</Application>
